    "def recur_fibo(n):\n",
    "    if n<=1:\n",
    "        return n\n",
    "    else:\n",
    "        return(recur_fibo(n-1) + recur_fibo(n-2))\n",
    "nterms = int(input(\"How many terms? \"))\n",
    "if nterms<=0:\n",
    "    print(\"Please enter positive number\")\n",
    "else:\n",
    "    for i in range(nterms):\n",
    "        print(recur_fibo(i))"
   ]
