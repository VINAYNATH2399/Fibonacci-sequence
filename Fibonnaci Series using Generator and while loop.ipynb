    "def fibonnaci():\n",
    "    a,b=0,1\n",
    "    while True:\n",
    "        yield a              #this function using in generator\n",
    "        a,b=b,a+b\n",
    "f1 = fibonnaci()\n",
    "print(next(f1))\n",
    "print(next(f1))\n",
    "print(next(f1))\n",
    "print(next(f1))\n",
    "print(next(f1))\n",
    "print(next(f1))\n",
    "print(next(f1))"
